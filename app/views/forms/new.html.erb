<div class="container w-100 background pb-5">

  <div class="mx-5 w-50">
    <h3 class="text-center pt-5 pb-3">Information</h3>
    <%= simple_form_for(@form) do |f| %>
      <%= f.input :presented_on, label: "Date" %> 
      <%= f.input :name, label: "Title" %>
      <%= f.submit 'Create Form', class: 'btn btn-click mt-3' %>
    <% end %>
  </div>

</div>